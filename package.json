  "dependencies": {
    "axios": "^0.20.0",
    "clsx": "^1.1.1",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.6.5",
    "d3-array": "^2.4.0",
    "d3-ease": "^1.0.6",
    "d3-interpolate": "^1.4.0",
    "history": "4.10.0",
